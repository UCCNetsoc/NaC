[http.routers]
    [http.routers.sysadmin-router]
      entrypoints = ["web-secure"]
      rule = "Host(`sysadmin.netsoc.co`)"
      middlewares = ["sysadmin=redirect"]
      service = "unused-service"

      {% if 'lxc' not in ansible_virtualization_type %}
      [http.routers.sysadmin.tls]
          certResolver = "letsencrypt"
      {% endif %}

[http.middlewares]
  [http.middlewares.sysadmin-redirectRegex]
    regex = "^(.*)"
    replacement = "https://forms.gle/y52GjHmq59XYMEQT9"

[htp.services]
  [http.services.unused-service.loadBalancer]
    [[http.services.unused-service.loadBalancer.servers]]
      url = "https://google.com"